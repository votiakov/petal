apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: legendary
  annotations:
    kubernetes.io/ingress.class: nginx
spec:
  rules:
  - host: legendary-demo.mythicinsight.com
    http:
      paths:
      - backend:
          service:
            name: legendary
            port:
              number: 80
        path: /
        pathType: Prefix
